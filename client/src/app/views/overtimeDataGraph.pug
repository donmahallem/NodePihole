.box
	.box-header.with-border
		h3.box-title Queries over time
	.box-body
		.chart
			canvas(class="chart chart-line",chart-data="$ctrl.data",chart-labels="$ctrl.labels",chart-colors="$ctrl.colors",chart-options="$ctrl.options",chart-click="$ctrl.onGraphClick",chart-dataset-override="$ctrl.datasetsOverride",width="800", height="250")
	.overlay(ng-show="$ctrl.isLoading")
		i.fa.fa-refresh.fa-spin